{
  "name": "electron-starter",
  "productName": "Electron Starter",
  "version": "0.4.0",
  "description": "Electron app starter with React, Tailwind CSS, and shadcn/ui",
  "main": "dist/main/index.js",
  "private": true,
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "start": "electron-vite dev",
    "package": "electron-vite build && electron-builder build --dir",
    "make": "electron-vite build && electron-builder build",
    "make:win": "electron-vite build && electron-builder build --win",
    "make:mac": "electron-vite build && electron-builder build --mac",
    "make:linux": "electron-vite build && electron-builder build --linux",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "release": "dotenv -- release-it",
    "release:patch": "dotenv -- release-it patch",
    "release:minor": "dotenv -- release-it minor",
    "release:major": "dotenv -- release-it major",
    "bump-shadcn-components": "ts-node ./scripts/bump-shadcn-components.ts",
    "init-project": "node scripts/init.mjs",
    "generate-icons": "node scripts/generate-icons.mjs"
  },
  "author": "Corey Peruffo <cperuffo3@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@electron/fuses": "~1.8.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.0",
    "@inquirer/prompts": "^8.1.0",
    "@release-it/conventional-changelog": "^10.0.4",
    "@tailwindcss/node": "^4.1.18",
    "@tanstack/router-plugin": "^1.144.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/semver": "^7.7.1",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "^11.0.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "electron-devtools-installer": "^4.0.0",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-tailwind-canonical-classes": "^1.1.0",
    "globals": "^17.0.0",
    "icon-gen": "^5.0.0",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "release-it": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0",
    "vite-plugin-static-copy": "^3.1.4"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.8",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@orpc/client": "^1.10.4",
    "@orpc/server": "^1.13.1",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.13",
    "@tanstack/react-router": "^1.144.0",
    "@tanstack/react-router-devtools": "^1.144.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "electron-updater": "^6.7.3",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.3",
    "react-number-format": "^5.4.4",
    "shadcn": "^3.6.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.2.1"
  }
}
